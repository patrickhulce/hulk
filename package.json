{
  "name": "@patrickhulce/scripts",
  "version": "0.0.0",
  "description": "Collection of useful scripts.",
  "bin": {
    "hulk": "./bin/hulk.js"
  },
  "scripts": {
    "test": "lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickhulce/hulk.git"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickhulce/hulk/issues"
  },
  "homepage": "https://github.com/patrickhulce/hulk#readme",
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "eslint": {
      "rules": {
        "func-names": [
          "error",
          "always"
        ],
        "operator-linebreak": 0,
        "indent": 0,
        "quote-props": 0,
        "no-extra-parens": 0,
        "no-console": 0,
        "space-before-function-paren": 0,
        "curly": 0
      }
    }
  },
  "devDependencies": {
    "@patrickhulce/lint": "^3.0.2",
    "@types/shelljs": "^0.8.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "conf": "^2.0.0",
    "conventional-changelog-writer": "^5.0.1",
    "conventional-commits-parser": "^3.0.0",
    "get-stream": "^3.0.0",
    "git-url-parse": "^14.0.0",
    "inquirer": "^6.0.0",
    "into-stream": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jimp": "^0.22.12",
    "lodash": "^4.17.5",
    "pkgfiles": "^2.3.2",
    "semver": "^5.5.0",
    "shelljs": "^0.8.1",
    "yargs": "^11.0.0"
  },
  "overrides": {
    "minimist": "^1.2.6"
  }
}
